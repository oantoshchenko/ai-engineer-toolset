# Tools Environment Configuration
# Copy this file to .env and fill in your values
# .env is gitignored and will not be committed

# ElevenLabs TTS (for mcp/elevenlabs-tts.sh)
ELEVENLABS_API_KEY=

# Obsidian Local REST API (for mcp/obsidian-edit.sh)
# Get the API key from Obsidian Settings > Local REST API
OBSIDIAN_API_KEY=
OBSIDIAN_BASE_URL=http://127.0.0.1:27123

# OpenMemory (optional, for authenticated setups)
# Leave empty for local unauthenticated access
OPENMEMORY_API_KEY=
OPENMEMORY_URL=http://localhost:8787

